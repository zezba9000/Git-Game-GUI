<UserControl x:Class="GitGUI.HistoryUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GitGUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Background="Red">
		<TreeView x:Name="treeView" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0" Height="127" Width="140"/>
		<Button x:Name="openGitkButton" Content="Open Gitk" HorizontalAlignment="Left" Margin="215,10,0,0" VerticalAlignment="Top" Width="75" Click="openGitkButton_Click"/>
	</Grid>
</UserControl>
